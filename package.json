{
  "name": "mako-browser",
  "version": "0.18.1",
  "description": "A mako plugin bundle for building a front-end project.",
  "keywords": [
    "mako-plugin",
    "mako-plugin-bundle",
    "browser",
    "front-end",
    "css",
    "js"
  ],
  "repository": "makojs/browser",
  "license": "MIT",
  "peerDependencies": {
    "mako": "^1.0.0"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "defaults": "^1.0.3",
    "mako-buffer": "^1.0.0",
    "mako-copy": "^1.0.0",
    "mako-css": "^1.0.0",
    "mako-html": "^1.0.0",
    "mako-js": "^1.0.0",
    "mako-output": "^1.0.0",
    "mako-sourcemaps": "^1.0.0",
    "mako-stat": "^1.0.0",
    "mako-symlink": "^1.0.0",
    "mako-write": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "chai-as-promised": "^5.1.0",
    "istanbul": "^0.4.4",
    "istanbul-coveralls": "^1.0.3",
    "mako": "^1.0.0",
    "mocha": "^3.0.0",
    "rimraf": "^2.4.3",
    "standard": "^8.0.0"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "test": "istanbul cover _mocha",
    "coveralls": "istanbul-coveralls",
    "preversion": "npm test",
    "version": "git-changelog -t $npm_package_version && git add HISTORY.md",
    "postversion": "git push && git push --tags"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**"
    ]
  }
}
